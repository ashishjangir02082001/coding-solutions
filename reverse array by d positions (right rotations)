// We will rotate an array by d positions to the right.
//C++ code goes here...

#include <iostream>

using namespace std;

void rotate(int arr[],int n, int d)
{
    int temp=arr[n-1];
    for(int i=n-2;i>=0;i--)
    {
        arr[i+1]=arr[i];
    }
    arr[0]=temp;
}

int main()
{
    int arr[] = { 1, 2, 3, 4, 5, 6, 7,10,34,21}; 
    int n = sizeof(arr) / sizeof(arr[0]); 
    int d = 4; 
    
    // we will rotate the array one by one (right rotation)
    for(int i=0;i<d;i++)
    {
        rotate(arr,n,d);
    }

    for(int i=0;i<n;i++)
    cout<<arr[i]<<" ";
    return 0;
}
